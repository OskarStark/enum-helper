{
    "name": "oskarstark/enum-helper",
    "type": "library",
    "description": "This library provides helpers for several enum operations",
    "keywords": [
        "enum"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "ergebnis/php-cs-fixer-config": "^5.16",
        "phpstan/phpstan": "^1.11.8",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OskarStark\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OskarStark\\Enum\\Tests\\": "tests/"
        }
    }
}
